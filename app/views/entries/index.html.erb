




<div class="btn btn-primary">
  <%= link_to 'Nouveau Flux', new_feed_path,  {:style=>'color:#FFFFFF;', :class => "css_class"} %>
</div>
<div class="page-header">
  <h1>Mes Flux RSS</h1>
</div>

<div id="static-pagination">
  <%= will_paginate %>
</div>

<div id="my-entries">
  <div class="page-delimiter first-page" data-page="<%= params[:page] || 1 %>"></div>
  <%= render @entries %>
</div>

<% if @entries.next_page %>
  <div id="infinite-scrolling">
    <%= will_paginate %>
  </div>
<% end %>